<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<workflow-app xmlns="uri:oozie:workflow:0.5" name="copy_file_workflow">
    <start to="Move_Data"/>
    <action name="Move_Data">
        <fs>
            <name-node>${nameNode}</name-node>
            <move source="/data/data_in/date=${date}" target="/data/data_out/date=${date}"/>
        </fs>
        <ok to="end"/>
        <error to="kill"/>
    </action>
    <kill name="kill">
        <message>${wf:errorMessage(wf:lastErrorNode())}</message>
    </kill>
    <end name="end"/>
</workflow-app>
